{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/vog-productions-slider-item",
	"version": "0.1.0",
	"title": "Producties slider Item",
	"category": "theaterkoorvog-blocks",
	"icon": "admin-home",
