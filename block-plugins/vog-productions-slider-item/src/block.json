		"mediaUrl": {
			"type": "string",
			"default": ""
		},
		"mediaWidth": {
			"type": "number",
			"default": 0
		},
		"mediaHeight": {
			"type": "number",
			"default": 0
		}
	},
	"textdomain": "vog-productions-slider-item",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
