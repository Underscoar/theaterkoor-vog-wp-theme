	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"default": "Slide titel"
		},
		"year": {
			"type": "string",
			"default": "2024"
		},
		"link": {
			"type": "string",
			"default": "#"
		},
		"content": {
			"type": "string",
			"default": "Inhoud"
		},
		"mediaId": {
			"type": "number",
			"default": 0
