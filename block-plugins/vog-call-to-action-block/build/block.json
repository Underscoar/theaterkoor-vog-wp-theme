  "name": "create-block/vog-call-to-action-block",
  "version": "0.1.0",
  "title": "Standaard call to action block",
  "category": "theaterkoorvog-blocks",
  "icon": "admin-home",
  "description": "Standaard blok voor het weergeven van een call to action.",
  "example": {},
  "supports": {
    "html": false,
    "align": true
